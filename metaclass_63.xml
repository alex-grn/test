			<A id="ELECTCAMPAIGNID"/>
			<A id="LEVELELCAMPAIGN" labelwidth="400" readonly="true" virtual="true" formula="select t.LEVELELCAMPAIGN from ELECTCAMPAIGN t where t.id = @ELECTCAMPAIGNID"/>
		</TABLE>
		<TABLE id="SECTION1SOURCEDATA" name="Раздел 1. Исходные данные">

<SYSTEM id="63" name="Учет расходов на выборы 3">
	<MODULES id="Modules" name="Модули"/>
	<GROUPS_MENU name="Группы меню"/>
		<MENU name="Справочники по отчётности">
			<MI name="Показатели отчётности" formlist="ReportingIndicators"/>
		</MENU>
		<MENU name="Отчётность УИК">
			<MI name="Отчётность УИК" formlist="AccountabilityUik"/>
			<MI name="Отчётность ТИК" formlist="AccountabilityTik"/>
			<MI name="Отчётность ИКСРФ" formlist="AccountabilityIksrf"/>
		</MENU>
	<ATTRIBUTES name="Атрибуты">
	</ATTRIBUTES>
	<GROUPS_ATTRIBUTES name="Группы атрибутов"/>
	<TABLES name="Таблицы">
		
		<COMMENT name="Редакция форм, верхнее расположение"/>
		<TABLE id="WORDINGOFTHEFORMS" name="Редакции форм">
			<A id="NAME" name="Наименование" caption="Наименование" labelwidth="150" type="string" nulls="false" local="true"/>
			<A id="CODE" name="Код" caption="Код" labelwidth="600" type="string" nulls="false" local="true"/>
			<A id="REVISIONDATE" name="Дата редакции" caption="Дата редакции" labelwidth="600" readonly="true" type="date" nulls="false" formula="now()" local="true"/>
		</TABLE>
		<COMMENT name="Показатели форм, нижнее расположение"/>
		<TABLE id="MEASURESOFSHAPE" name="Показатели формы" >
			<A id="NAME" name="Наименование показателя" caption="Наименование показателя" labelwidth="150" type="string" nulls="false" local="true"/>
			<A id="CODE" name="Код строки формы" caption="Код" labelwidth="600" type="string" nulls="false" local="true"/>
			<A id="CONTROLOFFILLING" name="Контроль заполнения показателя" caption="Контроль заполнения показателя" labelwidth="150" type="list" nulls="false" local="true">
				<AI id="1" name="УИК"/>
				<AI id="2" name="ТИК"/>
				<AI id="3" name="ТИК с полномочиями ОИК"/>
				<AI id="4" name="ОИК"/>
				<AI id="5" name="ИКСРФ"/>
			</A>
		</TABLE>
		
		<COMMENT name="Отчётность УИК, верхнее расположение"/>
		<TABLE id="ACCOUNTABILITYUIK" name="Отчёты УИК">
			<A id="NAME" name="Наименование" caption="Наименование" labelwidth="150" type="string" nulls="false" local="true"/>
			<A id="CODE" name="Код" caption="Код" labelwidth="400" type="string" nulls="false" local="true"/>
			<A id="REVISIONDATE" name="Дата отчёта" caption="Дата отчёта" labelwidth="400" readonly="true" type="date" nulls="false" formula="now()" local="true"/>
			<A id="ELECTCAMPAIGNID"/>
			<A id="LEVELELCAMPAIGN" labelwidth="350" readonly="true" virtual="true" formula="select t.LEVELELCAMPAIGN from ELECTCAMPAIGN t where t.id = @ELECTCAMPAIGNID"/>
			<!-- <A id="LEVELELCAMPAIGN" name="Уровень избирательной кампании" caption="Уровень избирательной кампании" type="list" nulls="false"> -->
				<!-- <AI id="territory" name="Муниципальная избирательная кампания"/> -->
				<!-- <AI id="region" name="Региональная избирательная кампания"/> -->
				<!-- <AI id="central" name="Федеральная избирательная кампания"/> -->
			<!-- </A> -->
		</TABLE>
		<TABLE id="ACCOUNTABILITYUIK2" name="Спецификации">
			<A id="CODE" name="Код строки" caption="Код строки" labelwidth="600" type="int" len="10" nulls="false" local="true"/>
			<A id="NAME" name="Наименование" caption="Наименование" labelwidth="150" type="string" nulls="false" local="true"/>
		</TABLE>
		
		<COMMENT name="Отчётность ТИК, верхнее расположение"/>
		<TABLE id="ACCOUNTABILITYTIK" name="Отчёты ТИК">
			<A id="NAME" name="Наименование" caption="Наименование" labelwidth="150" type="string" nulls="false" local="true"/>
			<A id="CODE" name="Код" caption="Код" labelwidth="400" type="int" nulls="false" local="true"/>
			<A id="REVISIONDATE" name="Дата редакции" caption="Дата редакции" labelwidth="400" readonly="true" type="date" nulls="false" formula="now()" local="true"/>
		</TABLE>
		<TABLE id="ACCOUNTABILITYTIK2" name="Спецификации">
			<A id="CODE" name="Код строки" caption="Код строки" labelwidth="600" type="int" len="10" nulls="false" local="true"/>
			<A id="NAME" name="Наименование" caption="Наименование" labelwidth="150" type="string" nulls="false" local="true"/>
		</TABLE>
		
		<COMMENT name="Отчётность ИКСРФ, верхнее расположение"/>
		<TABLE id="ACCOUNTABILITYIKSRF" name="Отчёты ИКСРФ">
			<A id="NAME" name="Наименование" caption="Наименование" labelwidth="150" type="string" nulls="false" local="true"/>
			<A id="CODE" name="Код" caption="Код" labelwidth="400" type="int" nulls="false" local="true"/>
			<A id="REVISIONDATE" name="Дата редакции" caption="Дата редакции" labelwidth="400" readonly="true" type="date" nulls="false" formula="now()" local="true"/>
		</TABLE>
		<TABLE id="ACCOUNTABILITYIKSRF2" name="Спецификации">
			<A id="CODE" name="Код строки" caption="Код строки" labelwidth="600" type="int" len="10" nulls="false" local="true"/>
			<A id="NAME" name="Наименование" caption="Наименование" labelwidth="150" type="string" nulls="false" local="true"/>
		</TABLE>
		
	</TABLES>
	<ACTIONS name="Действия"/>
	<REPORTS name="Отчеты"/>
	<FORMS id="Forms" name="Формы">
		
		<FORM id="ReportingIndicators" name="Показатели отчётности">
			<GRID id="WordingOfTheForms" name="Редакции форм" table="WORDINGOFTHEFORMS"/>
			<TABS>
				<TAB name="Показатели формы">
					<GRID id="MeasuresOfShape" name="Показатели формы" table="MEASURESOFSHAPE"/>
				</TAB>
			</TABS>
		</FORM>
		
		<FORM id="AccountabilityUik" name="Отчётность УИК">
			<GRID id="AccountabilityUik" name="Отчётность УИК" table="ACCOUNTABILITYUIK"/>
			<TABS>
				<TAB name="Спецификации">
					<GRID id="AccountabilityUik2" name="Спецификации" table="ACCOUNTABILITYUIK2"/>
				</TAB>
			</TABS>
		</FORM>
		
		<FORM id="AccountabilityTik" name="Отчётность ТИК">
			<GRID id="AccountabilityTik" name="Отчётность ТИК" table="ACCOUNTABILITYTIK"/>
			<TABS>
				<TAB name="Спецификации">
					<GRID id="AccountabilityTik2" name="Спецификации" table="ACCOUNTABILITYTIK2"/>
				</TAB>
			</TABS>
		</FORM>
		
		<FORM id="AccountabilityXrf" name="Отчётность ИКСРФ">
			<GRID id="AccountabilityIksrf" name="Отчётность ИКСРФ" table="ACCOUNTABILITYIKSRF"/>
			<TABS>
				<TAB name="Спецификации">
					<GRID id="AccountabilityIksrf2" name="Спецификации" table="ACCOUNTABILITYIKSRF2"/>
				</TAB>
			</TABS>
		</FORM>
	</FORMS>
</SYSTEM>

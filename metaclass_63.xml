		</MENU>
		
		
	<ATTRIBUTES name="Атрибуты">
		<A id="WORDINGOFTHEFORMSID" name="Редакции форм" caption="Редакции форм" type="ref" table="WORDINGOFTHEFORMS" ret="CODE" form="WordingOfTheForms" nulls="true"/>
		<A id="ACCOUNTABILITYUIKID" name="Отчёты УИК" caption="Отчёты УИК" type="ref" table="ACCOUNTABILITYUIK" ret="CODE" form="AccountabilityUik" nulls="true"/>
		
		
		<A id="PROFESSIONDIRID" name="Профессии/Должности" caption="Профессии/Должности" type="ref" table="PROFESSIONDIR" ret="NAME" form="Professiondir" nulls="true"/>
		
		
		<A id="REPORTINGFORMID" name="Код формы" caption="Код формы" type="ref" table="REPORTINGFORM" ret="CODEFORM" form="ReportingForm" nulls="false"/>

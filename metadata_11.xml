	<ATTRIBUTES name="Атрибуты">
		<COMMENT name="Контрольная сумма служит для проверки того, что в записи ничего не поменялось. Вместо 'Какой то текст' надо написать выражение из полей, которые мы хотим контролировать на неизменяемость.  Например ' COALESCE(@CODE,'0')|| COALESCE(@NAME,'000') '"/>
		<A id="CHECK_SUM" name="Контрольная сумма" caption="Контрольная сумма" labelwidth="400" type="string" visible="form" readonly="true" formula="select md5('Какой то текст')"/>
		<COMMENT name="Ссылки на прочие справочники"/>
		<A id="OFFBUDGETKINDS_ID" name="Вид бюджета и внебюджетных средств" caption="Вид бюджета и внебюджетных средств" labelwidth="500" type="ref" table="OFFBUDGETKINDS" ret="CODE" form="OffBudgetKinds"/>

		<TABLE id="BKD" name="КДБ  Классификатор доходов бюджетов">
			<GA id="BKD_CODE_GR" name="Коды классификации доходов бюджетов" caption="Коды классификации доходов бюджетов">
				<A id="DIR_RECIPID" name="Код главного администратора доходов бюджета" caption="Код главного администратора доходов бюджета" labelwidth="430" width="30" nulls="true" visible="form"/>
				<GA id="BKD_VID_KDB" name="Вид доходов бюджетов" caption="Вид доходов бюджетов">
					<A id="INCOM_GROUP_ID" name="Группа" caption="Группа" labelwidth="50" width="50" nulls="true" visible="form"/>
					<A id="INCOM_SUB_GROUP_ID" name="Подгруппа" caption="Подгруппа" labelwidth="70" width="50" nulls="true" visible="form"/>
					<A id="INCOM_ARTICLE_ID" name="Cтатья" caption="Cтатья" labelwidth="50" width="50" nulls="true" visible="form"/>
					<A id="INCOM_SUB_ARTICLE_ID" name="Подстатья" caption="Подстатья" labelwidth="70" width="50" nulls="true" visible="form"/>
					<A id="BUDG_ELEMENT_ID" name="Код элемента доходов" caption="Код элемента доходов" labelwidth="420" nulls="true" visible="form"/>
				</GA>
				<GA id="INCOM_SUB_VID_KDB" name="Подвид доходов бюджета" caption="Подвид доходов бюджета">
					<A id="INCOM_SUB_VID_ID" name="Группа подвида доходов бюджета" caption="Группа подвида доходов бюджета" labelwidth="500" nulls="true" visible="form"/>
					<A id="ECON_CLASS_ID" name="Аналитическая группа подвида доходов бюджетов" caption="Аналитическая группа подвида доходов бюджетов" labelwidth="500" nulls="true" visible="form"/>
				</GA>
				<A id="CODE" name="Код КДБ" caption="Код классификатора доходов бюджетов" labelwidth="300" type="string" nulls="true" readonly="true" formula="COALESCE((select  v.CODE from DIR_RECIP v where v.id = @DIR_RECIPID),'000')||COALESCE((select  v.CODE from INCOM_GROUP v where v.id = @INCOM_GROUP_ID),'0')||COALESCE((select  v.CODE from INCOM_SUB_GROUP v where v.id = @INCOM_SUB_GROUP_ID),'00')||COALESCE((select  v.CODE from INCOM_ARTICLE v where v.id = @INCOM_ARTICLE_ID),'00')||COALESCE((select  v.CODE from INCOM_SUB_ARTICLE v where v.id = @INCOM_SUB_ARTICLE_ID),'000')||COALESCE((select  v.CODE from BUDG_ELEMENT v where v.id = @BUDG_ELEMENT_ID),'00')||COALESCE((select  v.CODE from INCOM_SUB_VID v where v.id = @INCOM_SUB_VID_ID),'0000')||COALESCE((select  v.CODE from ECON_CLASS v where v.id = @ECON_CLASS_ID),'000')" local="true"/>
			</GA>
			<A id="NAME" labelwidth="100"/>
			<GA id="DictionaryParam"/>
			<TI id="iBKD_UK" unique="CODE,BEGIN_DATE"/>
			<TI id="iBKD_ACTIVE" index="ACTIVE"/>
		</TABLE>
		<TABLE id="KBR" name="КРБ Классификатор расходов бюджетов">
			<GA name="Коды классификации расходов бюджетов" caption="Коды классификации расходов бюджетов">
				<A id="DIR_RECIPID" name="Код главного распорядителя бюджетных средств" caption="Код главного распорядителя бюджетных средств" labelwidth="500" visible="form"/>
				<A id="FUN_CLASSID" name="Код раздела и подраздела" caption="Код раздела и подраздела" labelwidth="500" visible="form"/>
				<A id="PURP_ARTICLEID" name="Код целевой статьи" caption="Код целевой статьи" labelwidth="500" visible="form"/>
				<A id="EXP_TYPEID" name="Код вида расходов" caption="Код вида расходов" labelwidth="500" visible="form"/>
				<A id="CODE" name="Код КРБ" caption="Код классификации расходов бюджетов" labelwidth="300" type="string" nulls="true" readonly="true" formula="COALESCE((select  v.CODE from DIR_RECIP v where v.id = @DIR_RECIPID),'000')||COALESCE((select  v.CODE from FUN_CLASS v where v.id = @FUN_CLASSID),'0000')||COALESCE((select  v.CODE from PURP_ARTICLE v where v.id = @PURP_ARTICLEID),'0000000000')||COALESCE((select  v.CODE from EXP_TYPE v where v.id = @EXP_TYPEID),'000')" value="00000000000000000000" local="true"/>
			</GA>
			<A id="NAME" labelwidth="100" formula="COALESCE(@NAME,(select  v.NAME from FUN_CLASS v where v.id = @FUN_CLASSID)||'. '||(select v.NAME from PURP_ARTICLE v where v.id = @PURP_ARTICLEID)||'. '||(select v.NAME from EXP_TYPE v where v.id = @EXP_TYPEID))"/>
			<GA id="DictionaryParam"/>
			<TI id="iKBR_UK" unique="CODE,BEGIN_DATE"/>
			<TI id="iKBR_ACTIVE" index="ACTIVE"/>
		</TABLE>

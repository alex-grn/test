			<A id="NOTE" name="Полное наименование" caption="Полное наименование"/>
			<A id="CODEGP" name="Код программы" caption="Программное (непрограммное) направление расходов" labelwidth="600" type="string" nulls="true" readonly="true" virtual="true" formula="substr(@CODE,1,2)" local="true"/>
			<A id="CODEPP" name="Код подпрограммы" caption="Код подпрограммы" labelwidth="600" type="string" nulls="true" readonly="true" virtual="true" formula="substr(@CODE,3,1)" local="true"/>
			<A id="CODEOM" name="Код основного мероприятия целевой программы" caption="Код основного мероприятия целевой программы" labelwidth="600" type="string" nulls="true" readonly="true" virtual="true" formula="substr(@CODE,4,2)" local="true"/>
			<A id="CODENA" name="Код направления расходов" caption="Код направления расходов" labelwidth="600" type="string" nulls="true" readonly="true" virtual="true" formula="substr(@CODE,6,5)" local="true"/>
			<GA id="DictionaryParam"/>
			<TI id="iPURP_ARTICLE_UK" unique="CODE,BEGIN_DATE"/>

	<GROUPS_ATTRIBUTES name="Группы атрибутов">
		<COMMENT name="Простой справочник с Полным и кратким наименованием"/>
		<GA id="DictionaryBC">
			<A id="CODE" labelwidth="500"/>
			<A id="NAME" name="Сокращенное наименование" caption="Сокращенное наименование"/>
			<A id="NOTE" name="Полное наименование" caption="Полное наименование"/>
		</GA>
		<GA id="DictionaryParam" caption="Период действия">
			<A id="BEGIN_DATE" name="Дата начала действия" caption="Дата начала действия" labelwidth="220" width="50" type="date" nulls="true" visible="form" formula="to_date('01.01.'||to_char(now(),'YYYY'),'dd.mm.yyyy')" local="true"/>
			<A id="END_DATE" name="Дата окончания действия" caption="Дата окончания действия" labelwidth="220" width="50" type="date" nulls="true" visible="form" local="true"/>
			<A id="ACTIVE" name="Актуальная запись" caption="Актуальная запись" labelwidth="220" type="boolean" nulls="false" value="true" local="true"/>
		</GA>
	</GROUPS_ATTRIBUTES>

		<GA id="DictionaryParam" name="Период действия" caption="Период действия">
			<A id="BEGIN_DATE" name="Дата начала действия" caption="Дата начала действия" labelwidth="220" width="50" type="date" nulls="true" formula="to_date('01.01.'||to_char(now(),'YYYY'),'dd.mm.yyyy')" local="true"/>
			<A id="END_DATE" name="Дата окончания действия" caption="Дата окончания действия" labelwidth="220" width="50" type="date" nulls="true" local="true"/>
			<A id="ACTIVE" name="Актуальная запись" caption="Актуальная запись" labelwidth="220" type="boolean" nulls="false" value="true" local="true"/>
		</GA>
	</GROUPS_ATTRIBUTES>

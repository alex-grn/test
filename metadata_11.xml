		<A id="BUDG_ELEMENT_ID" name="Элемент бюджета" caption="Элемент бюджета" labelwidth="150" type="ref" table="BUDG_ELEMENT" ret="CODE" form="BudgElement" nulls="false"/>
		<A id="INCOM_SUB_VID_ID" name="Группа подвида доходов" caption="Группа подвида доходов" labelwidth="150" type="ref" table="INCOM_SUB_VID" ret="CODE" form="IncomSubVid" nulls="false"/>
		<A id="KBR_ID" name="КРБ" caption="Классификатор расходов бюджетов" labelwidth="400" type="ref" table="KBR" ret="CODE" form="Kbr"/>
		<COMMENT name="Ссылки на Общероссийские классификаторы"/>
		<A id="OKFS_ID" name="ОКФС" caption="Общероссийский классификатор форм собственности (ОКФС)" labelwidth="500" type="ref" table="OKFS" ret="CODE" form="OKFS"/>
		<A id="OKOPF_ID" name="ОКОПФ" caption="Общероссийский классификатор организационно-правовых форм (ОКОПФ)" labelwidth="500" type="ref" table="OKOPF" ret="CODE" form="OKOPF"/>

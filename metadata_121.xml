					<A id="PER_BO_PLAN_VIRT" name="% принятия БО к плану" caption="% принятия БО к плану" labelwidth="150" width="35" type="int" visible="form" readonly="true" virtual="true" formula="round((@SUM_BO*100)/NULLIF(@SUM_PLAN_OUT,0))" local="true"/>
					<A id="SUM_BA_BO_VIRT" name="Разница между БА и принятыми БО" caption="Разница между БА и принятыми БО" labelwidth="300" width="65" type="number" len="18.2" visible="form" readonly="true" virtual="true" formula="(@SUM_SEP_BA+@SUM_COR_BA)-@SUM_BO" local="true"/>
					<A id="PER_BA_BO_VIRT" name="% принятия БО к БА" caption="% принятия БО к БА" labelwidth="150" width="35" type="int" visible="form" readonly="true" virtual="true" formula="round((@SUM_BO*100)/NULLIF((@SUM_SEP_BA+@SUM_COR_BA),0))" local="true"/>
				</TAB>
				<TAB name="Кассовое исполнение">

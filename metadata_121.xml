						<A id="PER_OUT_BA_VIRT" name="% распределено" caption="% распределено" labelwidth="200" width="50" type="int" visible="form" readonly="true" virtual="true" formula="round(((@SUM_OUTIN_LBO+@SUM_OUT03_LBO+@SUM_OUT14_LBO)*100)/NULLIF((@SUM_SEP_BA+@SUM_COR_BA),0))" local="true"/>
					</GA>
					<GA caption="Бюджетные ассигнования">
						<A id="SUM_OUTIN_BA" name="Перечислено на свой счет ПБС ассигнований" caption="Перечислено на свой счет ПБС ассигнований" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_OUT03_BA" name="Перечислено на счета ПБС ассигнований" caption="Перечислено на счета ПБС ассигнований" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_OUT14_BA" name="Перечислено на счета по переданным полномочиям ассигнований" caption="Перечислено на счета по переданным полномочиям ассигнований" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_OUT_BA_VIRT" name="Итого распределено ассигнований" caption="Итого распределено" labelwidth="400" type="number" len="18.2" visible="form" readonly="true" virtual="true" formula="@SUM_OUTIN_BA+@SUM_OUT03_BA+@SUM_OUT14_BA" local="true"/>
						<A id="SUM_REST_BA_VIRT" name="Нераспределенный остаток ассигнований" caption="Нераспределенный остаток" labelwidth="400" type="number" len="18.2" visible="form" readonly="true" virtual="true" formula="(@SUM_SEP_BA+@SUM_COR_BA)-(@SUM_OUTIN_BA+@SUM_OUT03_BA+@SUM_OUT14_BA)" local="true"/>
					</GA>
				</TAB>
				<TAB name="Распределение ПОФ">
					<GA caption="Предельные объемы финансирования">
						<A id="SUM_OUTIN_POF" name="Перечислено на свой счет ПБС ПОФ" caption="Перечислено на свой счет ПБС ПОФ" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_OUT03_POF" name="Перечислено на счета ПБС ПОФ" caption="Перечислено на счета ПБС ПОФ" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_OUT14_POF" name="Перечислено на счета по переданным полномочиям ПОФ" caption="Перечислено на счета по переданным полномочиям ПОФ" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_OUT_POF_VIRT" name="Распределено ПОФ" caption="Итого распределено" labelwidth="400" type="number" len="18.2" visible="true" readonly="true" virtual="true" formula="@SUM_OUTIN_POF+@SUM_OUT03_POF+@SUM_OUT14_POF" local="true"/>
						<A id="SUM_REST_POF_VIRT" name="Нераспределенный остаток ПОФ" caption="Нераспределенный остаток" labelwidth="400" type="number" len="18.2" visible="true" readonly="true" virtual="true" formula="@SUM_IN_POF-(@SUM_OUTIN_POF+@SUM_OUT03_POF+@SUM_OUT14_POF)" local="true"/>
					</GA>
				</TAB>

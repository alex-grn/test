				<MI name="Журнал операций по исполнению расходной части бюджета" formlist="Jurnalexp"/>
			</MENU>
		</MENU>
	</GROUPS_MENU>
	<ATTRIBUTES name="Атрибуты"/>
	<GROUPS_ATTRIBUTES name="Группы атрибутов"/>
	<TABLES name="Таблицы">
		<TABLE id="UFK_NOTIF" name="Казначейские уведомления">

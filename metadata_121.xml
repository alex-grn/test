					<A id="SUM_NEED" name="Согласованные потребности" caption="Согласованные потребности" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
					<A id="SUM_PLAN_OUT" name="Запланированные расходы" caption="Запланированные расходы" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
					<GA caption="Обязательства">
						<A id="SUM_PBO" name="Принимаемые бюджетные обязательства" caption="Принимаемые бюджетные обязательства" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>

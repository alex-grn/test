						<AI id="0" name="Черновик" actionlist="MakeEntryKU"/>
						<AI id="1" name="Отработан" actionlist="DeleteEntryKU" readonly="true"/>
					</A>
				</TAB>
				<TAB name="Ответственные исполнители">
					<A id="DOL_ISP" name="Должность ответственного исполнителя" caption="Должность ответственного исполнителя" labelwidth="350" type="string" visible="form" local="true"/>
					<A id="NAME_ISP" name="ФИО ответственного исполнителя" caption="ФИО ответственного исполнителя" labelwidth="350" type="string" visible="form" local="true"/>
					<A id="TEL_ISP" name="Телефон ответственного исполнителя" caption="Телефон ответственного исполнителя" labelwidth="350" type="string" visible="form" local="true"/>
					<A id="DOL_NOTD" name="Должность начальника отдела" caption="Должность начальника отдела" labelwidth="350" type="string" visible="form" local="true"/>
					<A id="NAME_NOTD" name="ФИО начальника отдела" caption="ФИО начальника отдела" labelwidth="350" type="string" visible="form" local="true"/>
					<A id="DOL_RUK" name="Должность руководителя (уполномоченного им лица)" caption="Должность руководителя (уполномоченного им лица)" labelwidth="350" type="string" visible="form" local="true"/>
					<A id="NAME_RUK" name="ФИО руководителя" caption="ФИО руководителя" labelwidth="350" type="string" visible="form" local="true"/>
					<A id="NOTE_KU" name="Специальные указания" caption="Специальные указания" labelwidth="350" type="string" visible="form" local="true"/>
					<A id="CODE_KU" name="Код специальных указаний" caption="Код специальных указаний" labelwidth="350" type="string" visible="form" local="true"/>
					<A id="DATE_POD" name="Дата подписания казначейского уведомления руководителем (уполномоченным лицом) ФК" caption="Дата подписания казначейского уведомления руководителем (уполномоченным лицом) ФК" labelwidth="550" type="date" visible="form" local="true"/>
					<A id="DATA_POD_NACH" name="Дата подписания казначейского уведомления начальником управления (отдела)" caption="Дата подписания казначейского уведомления начальником управления (отдела)" labelwidth="550" type="date" visible="form" local="true"/>
				</TAB>
			</TABS>
			<TI id="iUFK_NOTIF_UK" unique="NOM_KU, DATE_KU"/>
		</TABLE>
		<TABLE id="UFK_NOTIF_BA" name="Бюджетные ассигнования">
			<A id="UFK_NOTIFID" name="Казначейские уведомления" caption="Казначейские уведомления" labelwidth="300" parentkey="cascade" type="ref" table="UFK_NOTIF" ret="NOM_KU" form="Ufk_Notif" local="true"/>
			<A id="KBR_ID"/>
			<A id="SUM_BA_Y1" name="Сумма" caption="Сумма на текущий финансовый год" labelwidth="500" type="number" len="18.2" local="true"/>
			<A id="SUM_BA_Y2" name="Сумма 1" caption="Сумма на первый год планового периода" labelwidth="500" type="number" len="18.2" local="true"/>
			<A id="SUM_BA_Y3" name="Сумма 2" caption="Сумма на второй год планового периода" labelwidth="500" type="number" len="18.2" local="true"/>
			<A id="NOTE_BA_STR" name="Примечание" caption="Примечание" labelwidth="200" type="text" local="true"/>
			<TI id="iUFK_NOTIF_BA_ID" index="UFK_NOTIFID"/>
		</TABLE>
		<TABLE id="UFK_NOTIF_LBO" name="Лимиты бюджетных обязательств">
			<A id="UFK_NOTIFID" name="Казначейские уведомления" caption="Казначейские уведомления" labelwidth="300" parentkey="cascade" type="ref" table="UFK_NOTIF" ret="NOM_KU" form="Ufk_Notif" local="true"/>
			<A id="KBR_ID"/>
			<A id="CAPOBJECTS_ID"/>
			<A id="FAIP_CODE" name="Код  ФАИП" caption="Код объекта ФАИП (КМИ)" labelwidth="350" type="string" local="true"/>
			<A id="SUM_LBO_Y1" name="Сумма" caption="Сумма на текущий финансовый год" labelwidth="500" type="number" len="18.2" local="true"/>
			<A id="SUM_LBO_Y2" name="Сумма 1" caption="Сумма на первый год планового периода" labelwidth="500" type="number" len="18.2" local="true"/>
			<A id="SUM_LBO_Y3" name="Сумма 2" caption="Сумма на второй год планового периода" labelwidth="500" type="number" len="18.2" local="true"/>
			<A id="NOTE_LBO_STR" name="Примечание" caption="Примечание" labelwidth="200" type="text" local="true"/>
			<TI id="iUFK_NOTIF_LBO_ID" index="UFK_NOTIFID"/>
		</TABLE>
		<TABLE id="UFK_NOTIF_POF" name="Предельные объемы">
			<A id="UFK_NOTIFID" name="Казначейские уведомления" caption="Казначейские уведомления" labelwidth="300" parentkey="cascade" type="ref" table="UFK_NOTIF" ret="NOM_KU" form="Ufk_Notif" local="true"/>
			<A id="KBR_ID"/>
			<A id="SUM_POF_Y1" name="Сумма" caption="Сумма на текущий финансовый год" labelwidth="500" type="number" len="18.2" local="true"/>
			<A id="NOTE_POF_STR" name="Примечание" caption="Примечание" labelwidth="200" type="text" local="true"/>
			<TI id="iUFK_NOTIF_POF_ID" index="UFK_NOTIFID"/>
		</TABLE>
		<TABLE id="EXEC_BUD" name="Исполнение бюджета">
			<TABS>
				<TAB name="Бюджет">
					<A id="BUDGETS_ID" labelwidth="300" width="60" visible="form"/>
					<A id="YEAR_KU" name="Учетный год" caption="Учетный год" labelwidth="150" width="40" type="int" len="4" nulls="false" formula="to_char(now(),'YYYY')" local="true"/>
					<A id="NOTE" name="Примечание" caption="Примечание"/>
				</TAB>
				<TAB name="Поступления">
					<GA caption="Казначейские уведомления">
						<A id="SUM_IN_BA" name="Поступило ассигнований" caption="Поступило ассигнований" labelwidth="200" width="50" type="number" len="18.2" visible="form" readonly="true" formula="1000000*1.2" local="true"/>
						<A id="SUM_IN_LBO" name="Поступило ЛБО" caption="Поступило ЛБО" labelwidth="200" width="50" type="number" len="18.2" visible="form" readonly="true" formula="1000000*1" local="true"/>
						<A id="PER_IN_VIRT" name="% разблокировано" caption="% разблокировано" labelwidth="250" width="40" type="int" visible="form" readonly="true" virtual="true" formula="round((@SUM_IN_LBO*100)/NULLIF(@SUM_IN_BA,0))" local="true"/>
						<A id="SUM_IN_POF" name="Поступило ПОФ" caption="Поступило ПОФ" labelwidth="150" width="50" type="number" len="18.2" visible="true" readonly="true" formula="1000000" local="true"/>
					</GA>
					<GA caption="Лимиты бюджетных обязательств">
						<A id="SUM_SEP_LBO" name="Детализировано ЛБО" caption="Детализировано" labelwidth="200" width="50" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_COR_LBO" name="Корректировка ЛБО" caption="Корректировка" labelwidth="200" width="50" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_LBO_VIRT" name="Итого ЛБО" caption="ИТОГО" labelwidth="400" type="number" len="18.2" visible="form" readonly="true" virtual="true" formula="@SUM_SEP_LBO+@SUM_COR_LBO" local="true"/>
					</GA>
					<GA caption="Бюджетные ассигнования">
						<A id="SUM_SEP_BA" name="Детализировано ассигнований" caption="Детализировано" labelwidth="200" width="50" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_COR_BA" name="Корректировка ассигнований" caption="Корректировка" labelwidth="200" width="50" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_BA_VIRT" name="Итого ассигнований" caption="ИТОГО" labelwidth="400" type="number" len="18.2" visible="form" readonly="true" virtual="true" formula="@SUM_SEP_BA+@SUM_COR_BA" local="true"/>
					</GA>
				</TAB>
				<TAB name="Перечисления">
					<GA caption="Лимиты бюджетных обязательств">
						<A id="SUM_OUTIN_LBO" name="Перечислено на свой счет ПБС ЛБО" caption="Перечислено на свой счет ПБС ЛБО" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_OUT03_LBO" name="Перечислено на счета ПБС ЛБО" caption="Перечислено на счета ПБС ЛБО" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_OUT14_LBO" name="Перечислено на счета по переданным полномочиям ЛБО" caption="Перечислено на счета по переданным полномочиям ЛБО" labelwidth="550" type="number" len="18.2" visible="form" readonly="true" formula="1000000" local="true"/>
						<A id="SUM_OUT_LBO_VIRT" name="Итого распределено ЛБО" caption="Итого распределено" labelwidth="400" type="number" len="18.2" visible="form" readonly="true" virtual="true" formula="@SUM_OUTIN_LBO+@SUM_OUT03_LBO+@SUM_OUT14_LBO" local="true"/>
						<A id="SUM_REST_LBO_VIRT" name="Нераспределенный остаток ЛБО" caption="Нераспределенный остаток" labelwidth="400" type="number" len="18.2" visible="form" readonly="true" virtual="true" formula="(@SUM_SEP_LBO+@SUM_COR_LBO)-(@SUM_OUTIN_LBO+@SUM_OUT03_LBO+@SUM_OUT14_LBO)" local="true"/>

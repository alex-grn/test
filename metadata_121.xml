<SYSTEM id="121" name="Управление бюджетными данными">
	<MODULES id="Modules" name="Модули">
		<MODULE id="UBD" name="Управление бюджетными данными" menu="UBD"/>
	</MODULES>
	<GROUPS_MENU name="Группы меню">
		<MENU id="UBD" name="Меню модуля Управление бюджетными данными">
			<MENU name="Файл">
				<MI name="Загрузить конфигурацию" actionlist="ConfigLoad"/>
				<MI name="Выгрузить конфигурацию" actionlist="ConfigExport"/>
				<MI name="Параметры системы" formlist="Options"/>
				<MI name="Загруженные файлы" formlist="Filelinks"/>
				<MI name="Выйти" actionlist="Exit"/>
			</MENU>
			<MENU name="Документы">
				<MI name="Казначейские уведомления" formlist="Ufk_Notif"/>
			</MENU>
		</MENU>
	</GROUPS_MENU>
	<ATTRIBUTES name="Атрибуты"/>
	<GROUPS_ATTRIBUTES name="Группы атрибутов"/>
	<TABLES name="Таблицы">
		<TABLE id="UFK_NOTIF" name="Казначейские уведомления">
			<TABS>
				<TAB name="Основная информация">
					<A id="NOM_KU" name="Номер" caption="Номер казначейского уведомления" labelwidth="400" type="string" nulls="false" local="true"/>
					<A id="DATE_KU" name="Дата" caption="Дата формирования казначейского уведомления" labelwidth="600" type="date" nulls="false" formula="now()" local="true"/>
					<GA caption="Участник бюджетного процесса">
						<A id="KOD_UBP" name="Код УБП" caption="Код" labelwidth="50" width="30" type="string" local="true"/>
						<A id="NAME_UBP" name="Наименование УБП" caption="Наименование" labelwidth="100" width="70" type="string" visible="form" local="true"/>
					</GA>
					<GA caption="Главный распорядитель бюджетных средств">
						<A id="GLAVA_GRS" name="Глава" caption="Код" labelwidth="50" width="30" type="string" local="true"/>
						<A id="DIR_RECIPID" name="ГРБС" caption="Ссылка на справочник" labelwidth="200" width="70" visible="form"/>
					</GA>
					<A id="LS_GRS" name="Лицевой счет" caption="Лицевой счет" labelwidth="500" type="string" local="true"/>
					<GA caption="Орган федерального казначейства">
						<A id="CODE_TOFK" name="Код органа ФК" caption="Код" labelwidth="50" width="30" type="string" local="true"/>
						<A id="NAME_TOFK" name="Полное наименование" caption="Наименование" labelwidth="100" width="70" type="string" visible="form" local="true"/>
					</GA>
					<A id="DATE_VKU" name="Дата введения в действие" caption="Дата введения в действие казначейского уведомления" labelwidth="600" type="date" local="true"/>
					<A id="BUDGETS_ID" labelwidth="300" width="60" visible="form"/>
					<A id="YEAR_KU" name="Учетный год" caption="Учетный год" labelwidth="150" width="40" type="int" len="4" nulls="false" value="2020" local="true"/>
					<A id="SECRET_KU" name="Признак секретности" caption="Признак секретности казначейского уведомления" labelwidth="400" type="list" visible="form" value="Не секретно" local="true">
						<AI id="Не секретно" name="Не секретно"/>
						<AI id="ДСП" name="ДСП"/>
						<AI id="Секретно" name="Секретно"/>
						<AI id="Совершенно секретно" name="Совершенно секретно"/>
						<AI id="Особой важности" name="Особой важности"/>
					</A>
					<A id="TYPE_KBK" name="Тип КБК" caption="Тип бюджетной классификации" labelwidth="400" type="list" visible="form" value="0" local="true">
						<AI id="0" name="Расходы"/>
						<AI id="1" name="Источники внутреннего финансирования дефицита бюджета"/>
						<AI id="2" name="Источники внешнего финансирования дефицита бюджета"/>
					</A>
					<A id="GUID_FK" name="ГУИД" caption="Глобальный уникальный идентификатор" labelwidth="350" type="string" visible="form" readonly="true" local="true"/>
					<A id="STATUS" name="Статус" caption="Статус" labelwidth="500" type="list" visible="grid" value="0" local="true">
						<AI id="0" name="Черновик"/>
						<AI id="1" name="Отработан" readonly="true"/>
					</A>
				</TAB>
				<TAB name="Ответственные исполнители">
					<A id="DOL_ISP" name="Должность ответственного исполнителя" caption="Должность ответственного исполнителя" labelwidth="300" type="string" visible="form" local="true"/>
					<A id="NAME_ISP" name="ФИО ответственного исполнителя" caption="ФИО ответственного исполнителя" labelwidth="300" type="string" visible="form" local="true"/>
					<A id="TEL_ISP" name="Телефон ответственного исполнителя" caption="Телефон ответственного исполнителя" labelwidth="300" type="string" visible="form" local="true"/>
					<A id="DOL_NOTD" name="Должность начальника отдела" caption="Должность начальника отдела" labelwidth="300" type="string" visible="form" local="true"/>
					<A id="NAME_NOTD" name="ФИО начальника отдела" caption="ФИО начальника отдела" labelwidth="300" type="string" visible="form" local="true"/>
					<A id="DOL_RUK" name="Должность руководителя (уполномоченного им лица)" caption="Должность руководителя (уполномоченного им лица)" labelwidth="300" type="string" visible="form" local="true"/>
					<A id="NAME_RUK" name="ФИО руководителя" caption="ФИО руководителя" labelwidth="300" type="string" visible="form" local="true"/>
					<A id="NOTE_KU" name="Специальные указания" caption="Специальные указания" labelwidth="300" type="string" visible="form" local="true"/>
					<A id="CODE_KU" name="Код специальных указаний" caption="Код специальных указаний" labelwidth="300" type="string" visible="form" local="true"/>
					<A id="DATE_POD" name="Дата подписания казначейского уведомления руководителем (уполномоченным лицом) ФК" caption="Дата подписания казначейского уведомления руководителем (уполномоченным лицом) ФК" labelwidth="550" type="date" visible="form" local="true"/>
					<A id="DATA_POD_NACH" name="Дата подписания казначейского уведомления начальником управления (отдела)" caption="Дата подписания казначейского уведомления начальником управления (отдела)" labelwidth="550" type="date" visible="form" local="true"/>
				</TAB>
			</TABS>
			<TI id="iUFK_NOTIF_UK" unique="NOM_KU, DATE_KU"/>
		</TABLE>
		<TABLE id="UFK_NOTIF_BA" name="Бюджетные ассигнования">
			<A id="UFK_NOTIFID" name="Казначейские уведомления" caption="Казначейские уведомления" labelwidth="300" parentkey="cascade" type="ref" table="UFK_NOTIF" ret="NOM_KU" form="Ufk_Notif" local="true"/>
			<A id="KBR_ID"/>
			<A id="SUM_BA_Y1" name="Сумма" caption="Сумма на текущий финансовый год" labelwidth="500" type="number" local="true"/>
			<A id="SUM_BA_Y2" name="Сумма 1" caption="Сумма на первый год планового периода" labelwidth="500" type="number" local="true"/>
			<A id="SUM_BA_Y3" name="Сумма 2" caption="Сумма на второй год планового периода" labelwidth="500" type="number" local="true"/>
			<A id="NOTE_BA_STR" name="Примечание" caption="Примечание" labelwidth="200" type="text" local="true"/>
			<TI id="iUFK_NOTIF_BA_ID" index="UFK_NOTIFID"/>
		</TABLE>
		<TABLE id="UFK_NOTIF_LBO" name="Лимиты бюджетных обязательств">
			<A id="UFK_NOTIFID" name="Казначейские уведомления" caption="Казначейские уведомления" labelwidth="300" parentkey="cascade" type="ref" table="UFK_NOTIF" ret="NOM_KU" form="Ufk_Notif" local="true"/>
			<A id="KBR_ID"/>
			<A id="CAPOBJECTS_ID"/>
			<A id="FAIP_CODE" name="Код  ФАИП" caption="Код объекта ФАИП (КМИ)" labelwidth="350" type="string" local="true"/>
			<A id="SUM_LBO_Y1" name="Сумма" caption="Сумма на текущий финансовый год" labelwidth="500" type="number" local="true"/>
			<A id="SUM_LBO_Y2" name="Сумма 1" caption="Сумма на первый год планового периода" labelwidth="500" type="number" local="true"/>
			<A id="SUM_LBO_Y3" name="Сумма 2" caption="Сумма на второй год планового периода" labelwidth="500" type="number" local="true"/>
			<A id="NOTE_LBO_STR" name="Примечание" caption="Примечание" labelwidth="200" type="text" local="true"/>
			<TI id="iUFK_NOTIF_LBO_ID" index="UFK_NOTIFID"/>
		</TABLE>
		<TABLE id="UFK_NOTIF_POF" name="Предельные объемы">
			<A id="UFK_NOTIFID" name="Казначейские уведомления" caption="Казначейские уведомления" labelwidth="300" parentkey="cascade" type="ref" table="UFK_NOTIF" ret="NOM_KU" form="Ufk_Notif" local="true"/>
			<A id="KBR_ID"/>
			<A id="SUM_POF_Y1" name="Сумма" caption="Сумма на текущий финансовый год" labelwidth="500" type="number" local="true"/>
			<A id="NOTE_POF_STR" name="Примечание" caption="Примечание" labelwidth="200" type="text" local="true"/>
			<TI id="iUFK_NOTIF_POF_ID" index="UFK_NOTIFID"/>
		</TABLE>
	</TABLES>
	<ACTIONS name="Действия">
		<ACTION id="MakeEntryKU" name="Отработать" type="procedure" procedure="p_ubd_makeentryku"/>
		<ACTION id="DeleteEntryKU" name="Снять отработку" type="procedure" procedure="p_ubd_deleteentryku"/>
	</ACTIONS>
	<REPORTS name="Отчеты"/>
	<FORMS id="Forms" name="Формы">
		<FORM id="Ufk_Notif" name="Казначейские уведомления">
			<GRID id="Ufk_Notif" name="Казначейские уведомления" table="UFK_NOTIF" actionlist="MakeEntryKU;DeleteEntryKU"/>
			<TABS>
				<TAB name="Бюджетные ассигнования">
					<GRID id="Ufk_Notif_Ba" name="Бюджетные ассигнования" table="UFK_NOTIF_BA"/>
				</TAB>
				<TAB name="Лимиты бюджетных обязательств">
					<GRID id="Ufk_Notif_Lbo" name="Лимиты бюджетных обязательств" table="UFK_NOTIF_LBO"/>
				</TAB>
				<TAB name="Предельные объемы">
					<GRID id="Ufk_Notif_Pof" name="Предельные объемы" table="UFK_NOTIF_POF"/>
				</TAB>
			</TABS>
		</FORM>
	</FORMS>
</SYSTEM>

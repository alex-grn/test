				<A id="BALANCE_SUM_VIRT" name="Сумма остатка на начало года" caption="Сумма остатка на начало года" labelwidth="550" type="number" len="18.2" visible="true" readonly="true" virtual="true" formula="select sum(BALANCE_SUM) from MBT_REPORT_REC where MBT_REPORTID=@ID" local="true"/>

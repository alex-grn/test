						<AI id="1" name="Реестр"/>
					</A>
					<A id="DISTRIB_NOM" name="Номер распоряжения" caption="Номер" labelwidth="50" width="43" type="string" nulls="false" formula="select coalesce(regexp_replace(max(lpad(distrib_nom,80,' ')), '[^0-9]', '', 'g')::int+1,1) from @TABLENAME where cid=0 and year_u = @YEAR_U" button="true" local="true"/>

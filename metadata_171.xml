	<GROUPS_ATTRIBUTES name="Группы атрибутов">
	</GROUPS_ATTRIBUTES>
	<TABLES name="Таблицы">
		<TABLE id="SUBJECTSDIR" name="Участники взаимодействия">
			<A id="CODE" name="Код" caption="Код" labelwidth="700" type="int" len="2" nulls="false" local="true"/>
			<A id="NAME" name="Наименование" caption="Наименование" labelwidth="120" type="string" nulls="false" local="true"/>
			<A id="GEOTERR" name="Субъект РФ" caption="Субъект РФ" labelwidth="120" type="string" ret="NAME" nulls="true" local="true"/>
			<A id="ORGTYPE" name="Тип организации" caption="Тип организации" labelwidth="120" type="list" nulls="false" value="01" local="true">
				<AI id="01" name="Территориальный орган"/>
				<AI id="02" name="ФОИВ"/>
			</A>
			<A id="HIGHER" name="Вышестоящий орган" caption="Вышестоящий орган" labelwidth="120" type="list" nulls="false" value="01">
				<AI id="01" name="Роструд"/>
				<AI id="02" name="Минтруд"/>
			</A>
			<TI id="SUBJECTSDIR_UK1" unique="NAME"/>
			<TI id="SUBJECTSDIR_UK2" unique="CODE"/>
		</TABLE>
		<TABLE id="DIVISION" name="Подразделения">
			<A id="SUBJECTSDIRID" parentkey="cascade"/>
			<A id="NAME" name="Сокращенное наименование" caption="Сокращенное наименование" labelwidth="165" type="string" nulls="false" local="true"/>
			<A id="DESCRIPTION" name="Полное наименование" caption="Полное наименование" labelwidth="165" type="text" nulls="false" local="true"/>
		</TABLE>
		<TABLE id="PERSON" name="Сотрудники">
			<TABS>
				<TAB name="Основная информация">
					<A id="SUBJECTSDIRID" name="Организация" caption="Организация" labelwidth="120" nulls="false"/>
					<A id="SURNAME" name="Фамилия" caption="Фамилия" labelwidth="120" type="string" visible="form" nulls="false" local="true"/>
					<A id="FIRSTNAME" name="Имя" caption="Имя" labelwidth="120" type="string" visible="form" nulls="false" local="true"/>
					<A id="MIDDLENAME" name="Отчество" caption="Отчество" labelwidth="120" type="string" visible="form" nulls="true" local="true"/>
					<A id="FIOVIRT" name="Сотрудник" labelwidth="120" type="string" visible="grid" virtual="true" formula="select trim(COALESCE(surname,'')||' '||COALESCE(firstname,'')||' '||COALESCE(middlename,'')) from person d where d.id = @ID ::bigint" local="true"/>
					<A id="BIRTHDATE" name="Дата рождения" caption="Дата рождения" labelwidth="650" type="date" nulls="false" local="true"/>
					<A id="SNILS" name="СНИЛС" caption="СНИЛС" labelwidth="120" type="string" format="DDD-DDD-DDD DD" nulls="false" local="true"/>
					<A id="POST" name="Должность" caption="Должность" labelwidth="120" type="string" nulls="false" local="true"/>
					<A id="DEPART" name="Подразделение" caption="Подразделение" labelwidth="120" type="ref" nulls="false" pid="SUBJECTSDIRID" table="DIVISION" ret="DESCRIPTION" local="true"/>
				</TAB>
				<TAB name="Контактная информация">
					<A id="PHONES" name="Телефон" caption="Телефон" labelwidth="110" type="string" format="DDDDDDDDDD" nulls="false" local="true"/>
					<A id="EMAIL" name="E-Mail" caption="E-Mail" labelwidth="110" type="string" nulls="false" local="true"/>
				</TAB>	
			</TABS>
			<TI id="PERSON_EMAIL_UK" name="E-Mail уже существует" unique="EMAIL"/>
			<TI id="PERSON_SNILS_UK" name="СНИЛС уже существует" unique="SNILS"/>
		</TABLE>	
		<TABLE id="PRIMAINFO" name="Первичная инфомация">
		<COMMENT name="Участник = Ссылка по уровню доступа из таблицы SUBJECTSDIR"/>
			<A id="SUBJECTSDIRID" labelwidth="110" nulls="false"/>
			<A id="ORGTYPEVIRT" name="Тип организации" caption="Тип организации" type="list" visible="grid" virtual="true" formula="select b.orgtype from subjectsdir b where b.id=@SUBJECTSDIRID ::bigint" local="true">
				<AI id="01" name="Территориальный орган"/>
				<AI id="02" name="ФОИВ"/>
			</A>
			<A id="HIGHERVIRT" name="Вышестоящий орган" caption="Вышестоящий орган" labelwidth="120" type="list" visible="grid" virtual="true" formula="select b.higher from subjectsdir b where b.id=@SUBJECTSDIRID ::bigint" local="true">
				<AI id="01" name="Роструд"/>
				<AI id="02" name="Минтруд"/>
			</A>
			<A id="GEOTERRVIRT" name="Субъект РФ" caption="Субъект РФ" labelwidth="120" type="string" visible="grid" virtual="true" formula="select b.geoterr from subjectsdir b where b.id=@SUBJECTSDIRID ::bigint" local="true"/>
		<COMMENT name="Сотрудник = Ссылка по уровню доступа из таблицы PERSON"/>
			<GA id="PERSONINFO" name="Сведения о сотруднике">
				<A id="PERSONVIRTID" labelwidth="110" nulls="false"/>
				<A id="SURNAMEVIRT" name="Фамилия" caption="Фамилия" labelwidth="120" type="string" nulls="true" visible="false" virtual="true" formula="select b.surname from person b where b.id=@PERSONVIRTID ::bigint" local="true"/>
				<A id="FIRSTNAMEVIRT" name="Имя" caption="Имя" labelwidth="120" type="string" nulls="true" visible="false" virtual="true" formula="select b.firstname from person b where b.id=@PERSONVIRTID ::bigint" local="true"/>
				<A id="MIDDLENAMEVIRT" name="Отчество" caption="Отчество" labelwidth="120" type="string" nulls="true" visible="false" virtual="true" formula="select b.middlename from person b where b.id=@PERSONVIRTID ::bigint" local="true"/>
				<A id="BIRTHDATEVIRT" name="Дата рождения" caption="Дата рождения" labelwidth="450" type="date" nulls="true" visible="false" virtual="true" formula="select b.birthdate from person b where b.id=@PERSONVIRTID ::bigint" local="true"/>
				<A id="SNILSVIRT" name="СНИЛС" caption="СНИЛС" labelwidth="110" type="string" nulls="true" visible="grid" virtual="true" formula="select b.snils from person b where b.id=@PERSONVIRTID ::bigint" local="true"/>
				<A id="POSTVIRT" name="Должность" caption="Должность" labelwidth="110" type="string" nulls="true" visible="grid" virtual="true" formula="select b.post from person b where b.id=@PERSONVIRTID ::bigint" local="true"/>
				<A id="DEPARTVIRT" name="Подразделение" caption="Подразделение" labelwidth="110" type="string" nulls="true" visible="true" virtual="true" formula="(select d.description from division d where id = (select b.depart from person b where b.id=@PERSONVIRTID ::bigint))" local="true"/>
			</GA>
			<GA id="CONTACTINFO" name="Контактная инфомация" caption="Контактная инфомация">
				<A id="PHONESVIRT" name="Телефон" caption="Телефон" type="string" nulls="true" virtual="true" formula="select b.phones from person b where b.id=@PERSONVIRTID ::bigint" local="true"/>
				<A id="EMAILVIRT" name="E-Mail" caption="E-Mail" labelwidth="100" type="string" nulls="true" virtual="true" formula="select b.email from person b where b.id=@PERSONVIRTID ::bigint" local="true"/>
			</GA>
			<GA id="ACCESSINFO" name="Сведения о доступе" caption="Сведения о доступе">	
				<A id="PERMISSIONSUSER" labelwidth="165" />
				<A id="NUMBERUSERLICENSES" name="Пользовательская лицензия" caption="Пользовательская лицензия" labelwidth="165" type="string" format="WWWWWWWWWWWW-WWWWWWWWWWWW-WWW" nulls="false" local="true"/>
				<A id="NUMBERARM" name="№ АРМ" caption="№ АРМ" labelwidth="165" type="string" nulls="true" local="true"/>
				<!-- <A id="VIPNET" name="№ ВипНет-сети" caption="№ ВипНет-сети" labelwidth="165" type="string" nulls="true" local="true"/> -->
				<A id="USERLOGINVIRT" name="Логин" caption="Логин" labelwidth="165" type="string" nulls="true" visible="grid" virtual="true" formula="select b.email from person b where b.id=@PERSONVIRTID ::bigint" local="true"/>
			</GA>
			<TI id="PRIMAINFO_LIC_PERMIS_UK" name="Данные полномочия пользователя уже существуют" unique="PERSONVIRTID,PERMISSIONSUSER"/>
			<TI id="PRIMAINFO_SUBJECT_IND" index="SUBJECTSDIRID,PERSONVIRTID,PERMISSIONSUSER"/>
		</TABLE>
		
		
